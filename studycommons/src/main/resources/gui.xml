  <rowsPerPage>15</rowsPerPage>
  <buttons>
    <name>OK,Cancel,Help</name>
  </buttons>
  <numberFormat pattern="###\,###.##"/>
  <brokers>
  	<server>
  		<name>1</name>
  		<ip>10.200.23.2</ip>
  		<port>8080</port>
  	</server>
  	<server>
  		<name>2</name>
  		<ip>10.200.23.2</ip>
  		<port>8081</port>
  	</server>
  	<server>
  		<name>3</name>
  		<ip>10.200.23.2</ip>
  		<port>8082</port>
  	</server>
  </brokers>
  <gui>
  	<windowManager config-class="org.yy.config.DefaultWindowManager"
  		closable="false" resizable="true" defaultWidth="400" defaultHeight="250"
  	>
  		<styleDefinition config-class="org.yy.config.WindowStyleDefinition " backColor="#ffffff"
  		foreColor="#0080ff" iconName="myicon"
  		></styleDefinition>
  	</windowManager>
  </gui>
</gui-definition>
