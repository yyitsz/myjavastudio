package org.yy.rules
import org.yy.studydrools.entity.RoolVO

rule "A stand alone rule"
	
	when
		$vo : RoolVO(stringValue == "Learning to drool",
					$booleanVal : booleanValue)
		eval( $booleanVal )
	then 
		System.out.println("first Rule");
		$vo.setStringValue("Done.");
		
end

